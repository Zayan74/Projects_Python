import turtle
import random
t1 = turtle.Turtle()
t2 = turtle.Turtle()
screen = turtle.Screen()
screen.bgcolor("white")
t1.color("red")
t2.color("Grey")
t1.penup()
t2.penup()
t1.speed(random.randint(3, 5))
t2.speed(random.randint(3, 5))
t1.setposition(-200, 0)
t2.setposition(-200, 20)
t1.forward(random.randint(100, 400))
t2.forward(random.randint(100, 400))
turtle.done()
