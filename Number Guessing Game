import random

number = random.randint(1,100)
guesses = 0
guess = 0
while guess != number:
    guess = int(input("What is your guess: "))
    guesses += 1
    if guess < number:
        print("Too low, guess again")
    elif guess > number:
        print("Too high, guess again")
    else:
        print("You guessed correct")
        print(f"It took you {guesses} guesses")
        break
