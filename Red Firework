import turtle
t = turtle.Turtle()
screen = turtle.Screen()
t.speed(0)
t.setposition(0, 0)
screen.bgcolor("black")
t.color("red")
t.left(90)
t.forward(50)
t.penup()
t.right(90)
t.forward(10)
t.left(90)
t.pendown()
