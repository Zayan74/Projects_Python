import turtle
t = turtle.Turtle()
screen = turtle.Screen()
t.speed(0)
t.setposition(0, 70)
screen.bgcolor("black")
t.color("red")
t.begin_fill()
t.circle(15)
t.end_fill()
t.penup()
t.setposition(0, 55)
t.pendown()
t.color("orange")
t.circle(30)
t.penup()
t.setposition(0, 40)
t.pendown()
t.color("yellow")
t.circle(45)
t.penup()
t.setposition(0, 25)
t.pendown()
t.color("red")
t.circle(60)
