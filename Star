import turtle
t = turtle.Turtle()
screen = turtle.Screen()
t.speed(0)
screen.bgcolor("black")
t.setposition(-50,0)
t.color("yellow")
t.forward(100)
t.penup()
t.setposition(0,-50)
t.pendown()
t.left(90)
t.forward(100)
t.penup()
t.setposition(-40, -40)
t.pendown()
t.right(45)
t.forward(110)
t.penup()
t.setposition(40, -40)
t.pendown()
t.left(90)
t.forward(110)
