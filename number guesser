import random
number_guess = random.randint(1,10000)
guess = None
while guess != number_guess:
    guess = int(input("Guess a number between 1-10000: "))
    if guess > number_guess:
        print("Too high, try again!")
    elif guess < number_guess:
        print("Too low, try again!")
    else:
        print("You got it!")

